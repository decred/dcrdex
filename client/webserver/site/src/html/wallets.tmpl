              <div class="flex-center flex-column pe-2">
                <span>[[[No]]]</span>
                <input data-tmpl="no" type="radio" value="no" class="form-check-input">
              </div>
              <div class="flex-center flex-column pe-2">
                <span>[[[Yes]]]</span>
                <input data-tmpl="yes" type="radio" value="yes" class="form-check-input">
              </div>
            </div>
          </div>
        </div>

        {{- /* TREASURY KEYS */ -}}
