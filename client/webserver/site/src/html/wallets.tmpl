    </form>

    {{- /* RECONFIGURE WALLET */ -}}
    <form class="d-hide" id="reconfigForm" autocomplete="off">
      <div class="form-closer"><span class="ico-cross"></span></div>
      <header>
        [[[Reconfigure]]]
