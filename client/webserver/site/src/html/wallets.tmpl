    {{- /* UNAPPROVE TOKEN VERSIONS TABLE FORM */ -}}
    <form class="d-hide" id="unapproveTokenTableForm">
      <div class="form-closer hoverbg"><span class="ico-cross"></span></div>
      <div class="px-2 py-1 text-center position-relative fs24">
