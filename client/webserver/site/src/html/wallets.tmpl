          <span class="d-flex justify-content-between w-100">
            <span>[[[block]]]</span>
            <div><span id="txDetailsBlockNumber"></span><div class="ico-spinner spinner d-hide fs-14 px-2 d-inline-block" id="blockNumberPending"></div></div>
          </span>
          <span class="d-flex justify-content-between w-100">
            <span>[[[timestamp]]]</span>
            <div><span id="txDetailsTimestamp"></span><div class="ico-spinner spinner d-hide fs-14 px-2 d-inline-block" id="timestampPending"></div></div>
          </span>
          <span class="d-flex justify-content-between w-100">
            <span>[[[tx_id]]]</span>
            <div><span id="txDetailsID" class="ease-color"></span><span id="copyTxIDBtn" class="copy-btn ico-copy mx-2 fs18 ease-color"></span></div>
          </span>
          <span id="txDetailsRecipientSection" class="d-flex justify-content-between w-100">
            <span>[[[recipient]]]</span>
            <div><span id="txDetailsRecipient" class="ease-color"></span><span id="copyRecipientBtn" class="copy-btn ico-copy mx-2 fs18 ease-color"></span></div>
          </span>
          <div id="txDetailsBondSection" class="w-100">
            <span class="d-flex justify-content-between w-100">
              <span>[[[bond_id]]]</span>
              <div><span id="txDetailsBondID" class="ease-color"></span><span id="copyBondIDBtn" class="copy-btn ico-copy mx-2 fs18 ease-color"></span></div>
            </span>
            <span class="d-flex justify-content-between w-100">
              <span>[[[locktime]]]</span>
              <span id="txDetailsBondLocktime" class="ease-color"></span>
            </span>
            <span id="txDetailsBondAccountIDSection" class="d-flex justify-content-between w-100">
              <span>[[[Account ID]]]</span>
              <div><span id="txDetailsBondAccountID" class="ease-color"></span><span id="copyBondAccountIDBtn" class="copy-btn ico-copy mx-2 fs18 ease-color"></span></div>
            </span>
          </div>
          <span id="txDetailsNonceSection" class="d-flex justify-content-between w-100">
            <span>[[[nonce]]]</span><span id="txDetailsNonce"></span>
          </span>
        </div>
      </div>
    </form>

    {{- /* START MIXER PASSWORD FORM */ -}}
    <form id="mixerPWForm" class="d-flex align-items-stretch flex-column p-0 m-4 mw-425">
      <div class="form-closer hoverbg p-2 mt-2"><span class="ico-cross"></span></div>
      <div class="flex-center fs16 px-3">
        [[[mixing_pw_prompt]]]
      </div>
      <div class="d-flex flex-column align-items-stretch mt-2">
        <div class="flex-center w-100">
          <div class="flex-column flex-center">
            <label for="mixerPWInput">[[[App Password]]]</label>
            <input type="password" id="mixerPWInput" class="form-control select p-2 text-center mt-1" autocomplete="off">
          </div>
        </div>
        <div id="mixerPWErr" class="p-2 flex-center errcolor fs16 d-hide"></div>
        <div id="mixerPWSubmit" class="feature flex-center m-2 p-3 rounded3">
          <span class="ico-arrowright me-1"></span>
          Start
        </div>
      </div>
    </form>

    {{- /* CSPP ADDRESS CONFIG */ -}}
    <form id="mixerConfigForm" class="d-flex align-items-stretch flex-column p-0 m-4 mw-425">
      <div class="form-closer hoverbg p-2 mt-2"><span class="ico-cross"></span></div>
      <div class="flex-center fs24 w-100 px-3">
        <span class="ico-secretagent fs35 me-1"></span>
        <span id="mixerEnableLabel">[[[Enable Privacy]]]</span>
        <span id="mixerConfigureLabel">[[[Configure Privacy]]]</span>
      </div>
      <div id="mixAddrDisplayBox" class="flex-center fs18 my-2 px-3">
        <span id="mixAddrDisplay"></span>
        <div id="mixAddrEdit" class="d-inline-block ico-edit fs14 p-2 hoverbg pointer"></div>
      </div>
      <div id="mixAddrInputBox" class="flex-center flex-column mt-2">
        <label for="mixAddrInput">[[[cspp_addr]]]</label>
        <input type="text" id="mixAddrInput" class="p-2">
        {{template "certPicker"}}
      </div>
      <div id="mixConfigErr" class="flex-center fs16 errcolor p-2"></div>
      <div id="mixConfigSubmit" class="feature flex-center m-2 p-3 rounded3 hoverbg pointer">[[[Submit]]]</div>
      <div id="mixerDisable" class="flex-center mx-2 mb-2 p-3 brdr rounded3 errcolor hoverbg pointer">[[[Disable Privacy]]]</div>
    </form>

    {{- /* CSPP INFO */ -}}
    <form id="mixingInfo" class="p-0 ps-3 mw-425">
      <div class="form-closer hoverbg p-2 mt-2"><span class="ico-cross"></span></div>
      <ul class="px-3 pb-3">
        <li>
          [[[privacy_intro]]]
        </li>
        <li>
          [[[cspp_how]]]
        </li>
        <li>
          [[[decred_privacy]]]
        </li>
        <li>
          [[[privacy_optional]]]
        </li>
      </ul>
    </form>

    {{- /* CSPP INFO */ -}}
    <form id="mixingOffConfirmationForm" class="p-0 mw-425">
      <div class="form-closer hoverbg p-2 mt-2"><span class="ico-cross"></span></div>
      <div class="flex-center fs20 w-100 px-3">
        Are you sure that you want to turn off the privacy engine?
      </div>
      <div class="flex-center fs16 w-100 px-3 mt-2">
        Newly received funds will not become available for
        use until privacy is turned back on.
      </div>
      <div id="mixingOffGo" class="flex-center m-2 p-3 brdr rounded3 hoverbg pointer errcolor">Turn it off</div>
      <div id="mixingOffNo" class="feature flex-center m-2 p-3 rounded3">Nevermind</div>
    </form>
  </div>
</div>
{{template "bottom"}}
{{end}}
