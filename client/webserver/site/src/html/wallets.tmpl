{{define "wallets"}}
{{template "top" .}}
<div id="main" data-handler="wallets" class="flex-grow-1 position-relative">
  <span class="me-2 connection ico-connection d-hide peers-table-icon" id="connectedIconTmpl"></span>
  <span class="me-2 text-danger ico-disconnected d-hide peers-table-icon" id="disconnectedIconTmpl"></span>
  <span class="me-2 text-danger ico-cross d-hide pointer peers-table-icon" id="removeIconTmpl"></span>
  <div id="content" class="fill-abs d-flex">
    {{- /* ASSET SELECT */ -}}
    <section id="assetSelect" class="w-auto d-flex align-items-stretch overflow-y-hidden hidden-overflow invisible">
      <div id="globalBalanceBox" class="flex-stretch-column pt-2 px-2 hoverbg pointer">
        <span class="grey fs16 lh1 mb-1">Holdings</span>
        <span class="d-flex align-items-end lh1">
          <span id="globalBalance" class="fs20"></span>
          <span class="fs18 grey ms-1">USD</span>
        </span>
        <div class="border-bottom mt-2"></div>
      </div>
      <div id="tickerBalsBox" class="flex-stretch-column border-bottom">
        <div id="tickerBalTmpl" class="flex-stretch-column pt-2 px-2 hoverbg pointer">
          <div class="d-flex justify-content-between align-items-start">
            <div class="flex-center me-4 lh1">
              <img data-tmpl="logo" class="mini-icon me-1">
              <span data-tmpl="ticker" class="ms-1 fs22"></span>
            </div>
            <div class="d-flex flex-column align-items-end">
              <span data-tmpl="bal" class="fs22 lh1"> <span class="grey me-1">—</span> </span>
              <div data-tmpl="fiatBox" class="d-flex align-items-end fs15 grey lh1 pt-1">
                <span data-tmpl="fiatBal"> <span class="me-1">—</span> </span>
                <span class="ms-1 fs12 grey">USD</span>
              </div>
            </div>
          </div>
          <div class="border-bottom mt-2"></div>
        </div>
      </div>
      <div id="iconSelectTmpl" class="icon-select d-flex align-items-stretch p-2 border-bottom">
        <div class="flex-center">
          <div class="position-relative overflow-visible">
            <img data-tmpl="img">
            <img data-tmpl="parentImg" class="d-hide">
          </div>
        </div>
        <div class="d-none d-md-flex flex-column justify-content-around ps-3 lh1">
          <span class="fs17 demi text-nowrap" data-tmpl="name"></span>
          <span data-tmpl="balanceBox" class="d-flex align-items-end d-hide">
            <span data-tmpl="balance"></span>
            <span class="fs14 grey ms-1" data-tmpl="unit"></span>
          </span>
          <span data-tmpl="fiatBox" class="fs14 grey d-hide">
            <span data-tmpl="fiat"></span>
            <span>USD</span>
          </span>
          <span class="fs15 lh1" data-tmpl="noWallet">[[[Create a Wallet]]]</span>
        </div>
      </div>
    </section>

    <div class="flex-grow-1 position-relative">
      <div id="tickerBox" class="scrollsmall fill-abs d-flex flex-wrap align-items-stretch">
        {{- /* WALLET DETAILS */ -}}
        <div id="walletDetailsBox" class="position-relative col-24 col-xl-12 col-xxl-9 flex-stretch-column invisible">
          <div class="column flex-stretch-column">

            {{- /* BALANCE, SEND & RECEIVE */ -}}
            <section id="balanceDetails">
              <div class="d-flex justify-content-between align-items-start p-3">
                <div class="flex-center">
                  <img data-logo class="large-icon">
                  <div id="assetName" data-asset-name class="fs24 ms-2 demi lh1"></div>
                </div>
                <div class="d-flex flex-column justify-content-end">
                  <div class="d-flex align-items-end lh1">
                    <span data-tmpl="balance" class="fs28 me-1"></span>
                    <span data-ticker class="fs20 grey"></span>
                  </div>
                  <div data-tmpl="fiatBalanceBox" class="mt-1 lh1 grey fs15 d-flex justify-content-end align-items-center">
                    ~ <span data-tmpl="fiatBalance" class="me-1"></span> USD
                  </div>
                </div>
              </div>
              <div id="balanceBreakdownBox" class="border-top px-2">
                <table class="compact row-border no-bottom-border">
                  <thead class="unbold fs15">
                    <tr>
                      <th class="multinetonly">Network</th>
                      <th class="multinetonly">Allocation</th>
                      <th>Available</th>
                      <th>Locked</th>
                      <th>Pending</th> <!-- immature -->
                      <th></th>
                    </tr>
                  </thead>
                  <tbody id="balanceBreakdown">
                    <tr id="blockchainBalanceTmpl">
                      <td class="multinetonly">
                        <div class="d-inline-flex align-items-center">
                          <img data-tmpl="chainLogo" class="micro-icon greyscale">
                          <span data-tmpl="chainName" class="ms-1"></span>
                        </div>
                      </td>
                      <td data-tmpl="allocation" class="multinetonly">—</td>
                      <td data-tmpl="avail">—</td>
                      <td data-tmpl="locked">—</td>
                      <td data-tmpl="immature">—</td>
                      <td>
                        <button data-tmpl="txsBttn" class="micro">Txs</button>
                        <button data-tmpl="createWalletBttn" class="micro">Create Wallet</button>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </div>
              <div id="sendReceiveBox" class="d-flex align-items-stretch border-top">
                <div id="receive" class="flex-grow-1 flex-center p-2 pointer hoverbg border-right">
                  <span class="ico-qrcode fs15 me-1"></span>
                  <span class="fs20">[[[Receive]]]</span>
                </div>
                <button id="bridge" class="flex-grow-1 flex-center p-2 noborder hoverbg border-right d-hide">
                  <span class="ico-exchange me-1"></span>
                  <span class="fs20">[[[Bridge]]]</span>
                </button>
                <button id="send" class="flex-grow-1 flex-center p-2 noborder feature">
                  <span class="ico-send me-1"></span>
                  <span class="fs20">[[[Send]]]</span>
                </button>
              </div>
              <div id="createWalletBox" class="p-3 border-top text-end">
                <button id="createWallet" class="large feature">[[[create_a_x_wallet]]]</button>
              </div>
            </section>

            {{- /* EXCHANGE RATE */ -}}
            <section class="flex-stretch-column">
              <div class="flex-center py-2 fs15 demi">Exchange Rate</div>
              <div class="mx-2 border-bottom"></div>
              <div class="flex-grow-1 flex-center py-2">
                <span class="fs16 mb-1 demi">$</span>
                <span id="feeStateXcRate" class="fs22 me-1 demi lh1"></span>
              </div>
            </section>

            {{- /* TRANSACTION FEES */ -}}
            <section id="txFeesBox" class="flex-stretch-column">
              <div class="flex-stretch-column">
                <div class="flex-center py-2 fs18 demi">Transaction Fees</div>
                <div class="mx-2 border-bottom"></div>
              </div>

              {{- /* SINGLE-NETWORK ASSET TX FEES */ -}}
              <div class="d-flex uninetonly">
                <div class="flex-grow-1 d-flex flex-column align-items-center p-2">
                  <span class="fs16 demi">Send</span>
                  <span id="feeStateSendFiat" class="flex-center lh1">
                    <span>
                      <span data-tmpl="lessThan" class="fs16 demi">&lt;</span>
                      <span class="fs16 mb-1">$</span>
                    </span>
                    <span data-tmpl="value" class="fs20"></span>
                  </span>
                  <div id="feeStateSendFees" data-conversion-value class="fs14 grey">
                    <span data-value></span>
                    <span>
                      <span data-unit-box><span data-unit></span></span>
                    </span>
                  </div>
                </div>
                <div class="my-2 border-right"></div>
                <div class="flex-grow-1 d-flex flex-column align-items-center p-2">
                  <span class="fs16 demi">Sell</span>
                  <span id="feeStateSellFiat" class="flex-center lh1">
                    <span>
                      <span data-tmpl="lessThan" class="fs16 demi">&lt;</span>
                      <span class="fs16 mb-1">$</span>
                    </span>
                    <span data-tmpl="value" class="fs20"></span>
                  </span>
                  <div id="feeStateSellFees" data-conversion-value class="fs14 grey">
                    <span data-value></span>
                    <span>
                      <span data-unit-box><span data-unit></span></span>
                    </span>
                  </div>
                </div>
                <div class="my-2 border-right"></div>
                <div class="flex-grow-1 d-flex flex-column align-items-center p-2">
                  <span class="fs16 demi">Buy</span>
                  <span id="feeStateBuyFiat" class="flex-center lh1">
                    <span>
                      <span data-tmpl="lessThan" class="fs16 demi">&lt;</span>
                      <span class="fs16 mb-1">$</span>
                    </span>
                    <span data-tmpl="value" class="fs20"></span>
                  </span>
                  <div id="feeStateBuyFees" data-conversion-value class="fs14 grey">
                    <span data-value></span>
                    <span>
                      <span data-unit-box><span data-unit></span></span>
                    </span>
                  </div>
                </div>
                <div class="my-2 border-right"></div>
                <div class="flex-grow-1 d-flex flex-column align-items-center p-2">
                  <span class="fs16 demi">Rate</span>
                  <div id="networkFeeRate" data-conversion-value class="flex-center">
                    <span data-value class="fs22 me-1"></span>
                    <div class="d-flex flex-stretch-column lh1 fs13">
                      <span data-unit-box class="text-center"><span data-unit></span></span>
                      <div data-denom class="border-top text-center"></div>
                    </div>
                  </div>
                </div>
              </div>

              {{- /* MULTI-NETWORK ASSET TX FEES */ -}}
              <div class="px-2 multinetonly">
                <table class="compact row-border no-bottom-border">
                  <thead class="unbold fs15">
                    <tr>
                      <th class="tokenonly">Network</th>
                      <th>Send</th>
                      <th>Buy</th>
                      <th>Sell</th>
                      <th>Rate</th>
                    </tr>
                  </thead>
                  <tbody id="netTxFees">
                    <tr id="netTxFeeTmpl">
                      <td class="tokenonly">
                        <div class="d-inline-flex align-items-center">
                          <img data-tmpl="chainLogo" class="micro-icon greyscale">
                          <span data-tmpl="chainName" class="ms-1"></span>
                        </div>
                      </td>
                      <td id="multiNetTxFeeTmpl">
                        <div class="d-flex flex-column align-items-start">
                          <!-- fiat part -->
                          <span data-tmpl="fiatUnits" class="lh1">
                            <span data-tmpl="lessThan">&lt;</span>
                            <span>$</span>
                            <span data-tmpl="value"></span>
                          </span>
                          <!-- chain units part -->
                          <div data-tmpl="chainUnits" data-conversion-value class="fs14 grey">
                            <span data-value></span>
                            <span>
                              <span data-unit-box><span data-unit></span></span>
                            </span>
                          </div>
                        </div>
                      </td>
                      <td id="multiNetFeeRateTmpl" data-conversion-value class="text-nowrap">
                        <span data-value class="me-1"></span>
                        <span data-unit-box><span data-unit></span></span>/<span data-denom></span>
                      </td>
                      <td>—</td> <!--dummies-->
                      <td>—</td>
                      <td>—</td>
                      <td>—</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </section>

            {{- /* WALLET CONFIGURATION */ -}}
            <section id="walletConfig">
              <!-- wallet type, wallet class, wallet status, -->
              <div class="d-flex justify-content-between align-items-center p-3 lh1">
                <div class="flex-center">
                  <img data-logo class="micro-icon me-1"> 
                  <span class="fs20 demi">Wallet Configuration</span>
                </div>
                <div class="flex-center fs18">
                  <div id="statusLocked" class="flex-center">
                    <span class="ico-locked fs14 me-1"></span>
                    <span>[[[:title:locked]]]</span>
                  </div>
                  <div id="statusReady" class="flex-center">
                    <span class="ico-unlocked fs14 me-1"></span>
                    <span>[[[:title:ready]]]</span>
                  </div>
                  <div id="statusOff" class="flex-center">
                    <span class="ico-sleeping fs14 me-1"></span>
                    <span>[[[:title:off]]]</span>
                  </div>
                  <div id="statusDisabled" class="flex-center grey">
                    <span class="ico-cross fs14 me-1"></span>
                    <span>[[[Disabled]]]</span>
                  </div>
                  <div id="statusSyncing" class="flex-center grey">
                    <span class="ico-robot fs14 me-1"></span>
                    <span>Syncing</span>
                  </div>
                </div>
              </div>
              <div class="d-flex">
                <div class="col-12 flex-stretch-column border-top p-2">
                  <div class="d-flex justify-content-between align-items-center">
                    <span>[[[Type]]]</span>
                    <span id="walletType" class="demi"></span>
                  </div>
                  <div class="d-flex justify-content-between align-items-center">
                    <span>Class</span>
                    <span id="blockchainClass" class="demi"></span>
                  </div>
                  <div class="flex-grow-1 flex-stretch-column justify-content-end pt-2">
                    <div class="d-flex align-items-stretch">
                      <button id="unlockBttn" class="flex-grow-1 small me-1">[[[Unlock]]]</button>
                      <button id="lockBttn" class="flex-grow-1 small me-1">[[[Lock]]]</button>
                      <button id="connectBttn" class="flex-grow-1 small me-1">[[[Connect]]]</button>
                      <button id="reconfigureBttn" class="flex-grow-1 small">
                        <span class="ico-settings"></span>
                        <span>[[[Configure]]]</span>
                      </button>
                    </div>
                    
                  </div>
                </div>
                {{- /* SYNC STATUS */ -}}
                <div class="col-12 flex-stretch-column border-top border-left p-2">
                  <div class="d-flex justify-content-between align-items-center">
                    <span>[[[Peer Count]]]</span>
                    <span id="peerCount" class="demi">—</span>
                  </div>
                  <div class="d-flex justify-content-between align-items-center">
                    <span>[[[Block Sync]]]</span>
                    <span id="syncProgress" class="demi">—</span>
                  </div>
                  <div class="d-flex justify-content-between align-items-center">
                    <span>Blocks</span>
                    <span id="syncHeight" class="demi">—</span>
                  </div>
                  <div id="txSyncBox" class="d-hide">
                    <span>[[[Balance Discovery]]]</span>
                    <span id="txProgress" class="demi text-end"></span>
                    <span id="txFindingAddrs" class="demi">
                      <div class="d-flex align-items-center justify-content-end">
                        <div class="fs10 ico-spinner spinner me-1"></div>
                        <span>[[[Finding Addresses]]]</span>
                      </div>
                    </span>
                  </div>
                  <div class="flex-grow-1 flex-stretch-column justify-content-end pt-2">
                    <button id="managePeers" class="small">[[[manage_peers]]]</button>
                  </div>
                </div>
              </div>
            </section>

            {{- /* STAKING */ -}}
            <section id="stakingBox" class="position-relative d-flex align-items-stretch border">
              <div class="flex-stretch-column flex-grow-1">
                <div class="d-flex align-items-center justify-content-start border-bottom px-3 py-2">
                  <span class="ico-ticket me-2 fs20"></span>
                  <span class="fs24">[[[Staking]]]</span>
                </div>
                <div id="stakingSummary" class="d-flex align-items-stretch flex-grow-1">
                  <div class="flex-stretch-column justify-content-center fs14 flex-grow-1 p-2">
                    <div class="d-flex justify-content-between align-items-stretch">
                      <div class="flex-center grey">[[[Active tickets]]]</div>
                      <div id="stakingTicketCount" class="flex-center demi"></div>
                    </div>
                    <div id="immatureTicketCountBox" class="d-flex justify-content-between align-items-stretch">
                      <div class="flex-center grey">[[[Immature tickets]]]</div>
                      <div id="immatureTicketCount" class="flex-center demi"></div>
                    </div>
                    <div id="queuedTicketCountBox" class="d-flex justify-content-between align-items-stretch">
                      <div class="flex-center grey">[[[Queued tickets]]]</div>
                      <div id="queuedTicketCount" class="flex-center demi"></div>
                    </div>
                    <div class="d-flex justify-content-between align-items-stretch">
                      <div class="flex-center grey">[[[Tickets bought]]]</div>
                      <div id="totalTicketCount" class="flex-center demi"></div>
                    </div>
                    <div class="d-flex justify-content-between align-items-stretch">
                      <div class="flex-center grey">[[[Total rewards]]]</div>
                      <div id="totalTicketRewards" class="flex-center demi"></div>
                    </div>
                    <div class="d-flex justify-content-between align-items-stretch">
                      <div class="flex-center grey">[[[Votes cast]]]</div>
                      <div id="totalTicketVotes" class="flex-center demi"></div>
                    </div>
                    <div id="vspDisplayBox" class="d-flex justify-content-between align-items-stretch">
                      <div class="flex-center grey">[[[VSP]]]</div>
                      <div id="vspDisplay" class="flex-center demi pointer hoverbg">
                        <span id="vspEditIcon" class="ico-edit me-2"></span>
                        <span id="vspURL"></span>
                      </div>
                    </div>
                    <div id="pickVSP" class="mt-3 flex-stretch-column justify-content-end">
                      <button id="showVSPs">[[[select_vsp_from_list]]]</button>
                    </div>
                  </div>
                  <div id="setVotes" class="flex-center p-3 flex-column border-start hoverbg pointer">
                    <div class="flex-center fs18">
                      <span class="fs22 ico-check"></span>
                      <span class="ms-2 fs18">[[[Set Votes]]]</span>
                    </div>
                    <hr class="dashed my-1 w-75">
                    <div class="flex-center flex-column fs14">
                      <span><span id="stakingAgendaCount"></span> [[[agendas]]]</span>
                      <span><span id="stakingTspendCount"></span> [[[treasury spends]]]</span>
                    </div>
                  </div>
                </div>
                <div id="stakingErr" class="text-center p-2 text-danger"></div>
                <div id="stakingRpcSpvMsg" class="flex-center py-1 px-2">
                  <small class="ico-info me-2"></small>[[[staking_unavailable_for_rpc]]]
                </div>
                {{- /* PURCHASE TICKETS */ -}}
                <div id="purchaseTicketsBox" class="w-100 d-flex align-items-stretch justify-content-stretch border-top p-2">
                  <button id="purchaseTicketsBttn" class="feature flex-grow-1 me-2">
                    <span><span class="ico-ticket me-1"></span> [[[Purchase Tickets]]]</span>
                  </button>
                  <button id="ticketHistory" class="flex-grow-1 ">
                    <span><span class="ico-textfile me-1"></span> [[[Tickets]]]</span>
                  </button>
                </div>
              </div>
              <div id="ticketPriceBox" class="flex-stretch-column border-start">
                <div class="flex-grow-1 flex-center flex-column p-3 border-bottom">
                  <span class="fs14 demi lh1 pb-1">[[[Ticket Price]]]</span>
                  <span class="d-flex align-items-end">
                    <span class="fs18" id="ticketPrice"></span>
                  </span>
                </div>
                <div class="flex-grow-1 flex-center flex-column p-3">
                  <span class="fs14 demi lh1 pb-1">[[[Vote Reward]]]</span>
                  <span class="d-flex align-items-end">
                    <span class="fs18" id="votingSubsidy"></span>
                  </span>
                </div>
              </div>
              <div id="purchaseTicketsErrBox" class="fill-abs flex-stretch-column d-hide pb-3">
                <div id="purchaseTicketsErrCloser" class="p-2 hoverbg pointer"><span class="ico-cross fs20"></span></div>
                <div id="purchaseTicketsErr" class="p-2 flex-grow-1 flex-center text-danger"></div>
              </div>
            </section>
            <section id="ticketsDisabled" class="w-100 p-2 flex-center fs14 grey border-bottom">
              <span>[[[staking_disabled]]]</span>
            </section>

            {{- /* MIXING */ -}}
            <section id="mixingBox" class="position-relative d-flex align-items-stretch flex-column border">
              <div class="w-100 d-flex align-items-stretch">
                <div class="p-2 flex-center fs35 ico-secretagent border-end"></div>
                <div class="flex-center flex-grow-1">
                  <div id="mixerOn" class="flex-center fs20 d-hide">
                    <span id="onIndicator" class="on-indicator on me-2"></span>
                    <span class="">[[[Privacy active]]]</span>
                  </div>
                  <div id="mixerOff" class="flex-center fs20">
                    <span id="onIndicator" class="on-indicator off me-2"></span>
                    <span class="">[[[Privacy off]]]</span>
                  </div>
                </div>
                <div id="privacyInfoBttn" class="flex-center p-3 fs24 ico-info border-start hoverbg pointer"></div>
                <div class="p-2 border-start flex-center">
                  <div id="toggleMixer" class="anitoggle big"></div>
                </div>
              </div>
              <div id="mixerLoading" class="fill-abs flex-center bodybg">
                <span>
                  <span class="ico-spinner spinner me-2"></span>
                  <span>[[[loading privacy status]]]</span>
                </span>
              </div>
              <div id="mixingErr" class="flex-center p-2 text-danger border-top d-hide"></div>
            </section>{{- /* END MIXING */ -}}

            {{- /* ASSET DOCUMENTATION */ -}}
            <div id="docs">
              {{template "dcrDocs"}}
              {{template "btcDocs"}}
              {{template "ethDocs"}}
              {{template "usdcDocs"}}
              {{template "usdtDocs"}}
              {{template "ltcDocs"}}
              {{template "dogeDocs"}}
              {{template "dashDocs"}}
              {{template "dgbDocs"}}
              {{template "zecDocs"}}
              {{template "firoDocs"}}
              {{template "bchDocs"}}
              {{template "polDocs"}}
            </div>
          </div>
        </div>{{/* END WALLET DETAILS */}}

        {{- /* SECOND COLUMN */ -}}
        <div id="secondColumn" class="position-relative col-24 col-xl-12 col-xxl-15 flex-stretch-column stylish-overflow-lg invisible">
          <div class="column flex-stretch-column">

            {{/* PENDING TRANSACTIONS */}}
            <section>
              <div class="flex-center py-3 fs18">
                <span id="noPendingTxs">no pending transactions</span>
                <span id="somePendingTxs">
                  <span id="pendingTxsCount"></span>
                  <span>pending transactions</span>
                  <span id="expandPendingTxs" class="p-1 pointer hoverbg fs11 ico-arrowdown"></span>
                </span>
              </div>
              <div id="pendingTxsListBox" class="d-hide px-2 pb-3">
                <table id="pendingTxsList" class="compact row-border border-top">
                  <thead class="unbold fs15">
                    <tr>
                      <th class="multinetonly">Network</th>
                      <th>[[[Type]]]</th>
                      <th class="d-none d-sm-table-cell">[[[ID]]]</th>
                      <th>[[[Age]]]</th>
                      <th class="text-end">[[[Amount]]]</th>
                      <th>Confirms</th>
                    </tr>
                  </thead>
                  <tbody id="pendingTxs">
                    <tr id="pendingTxTmpl">
                      <td class="multinetonly">
                        <img data-tmpl="chainLogo" class="micro-icon greyscale">
                        <span data-tmpl="chainName" class="ms-1"></span>
                      </td>
                      <td data-tmpl="type"></td>
                      <td class="d-none d-sm-table-cell">
                        <a data-tmpl="id" class="subtlelink"></a>
                      </td>
                      <td data-tmpl="age"></td>
                      <td data-tmpl="amount" class="text-end"></td>
                      <td data-tmpl="confirms"></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </section>

            {{/* MARKETS */}}
            <section id="marketsOverviewBox">
              <h4 class="m-3">[[[asset_name Markets]]]</h4>
              <div>
                <table class="row-border row-hover border-top">
                  <thead>
                    <tr>
                      <th>[[[Market]]]</th>
