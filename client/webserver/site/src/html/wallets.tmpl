          </tr>
        </thead>
        <tbody id="tokenVersionBody">
          <tr id="tokenVersionRow">
            <td data-tmpl="version"></td>
            <td data-tmpl="usedBy"></td>
            <td data-tmpl="remove"></td>
          </tr>
        </tbody>
      </table>
    </form>

    {{- /* UNAPPROVE SPECIFIC TOKEN VERSION FORM */ -}}
