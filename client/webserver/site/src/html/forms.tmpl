{{define "walletConfigTemplates"}}
<div class="d-flex align-items-center justify-content-between">
  <span data-tmpl="walletSettingsHeader" class="fs20">
    [[[Wallet Settings]]]
  </span>
  <div class="flex-center">
    <button data-tmpl="fileSelector" type="button" class="small">
      <span>[[[load from file]]]</span>
      <span class="ico-textfile ms-1 fs14"></span>
    </button>
    <input data-tmpl="fileInput" type="file" class="d-none" accept=".conf, .cfg, .cnf, .ini">
  </div>
</div>

<div data-tmpl="allSettings">
  <div data-tmpl="dynamicOpts" class="dynamicopts">
    <div data-tmpl="textInput" class="px-0 d-flex flex-column justify-content-end mt-2">
      <label class="small"> <span class="ico-info"></span></label>
      <input type="text">
    </div>
    <div data-tmpl="checkbox" class="d-flex align-items-center mt-2 text-nowrap">
      <div class="form-check">
        <input class="form-check-input" type="checkbox" value="">
        <label class="form-check-label fs14"> <span class="ico-info fs13"></span></label>
      </div>
    </div>
    <div data-tmpl="dateInput" class="px-0 d-flex flex-column justify-content-end mt-2">
      <label class="small"> <span class="ico-info"></span></label>
      <input type="date">
    </div>
  </div>
  <div data-tmpl="repeatableInput" class="w-100 mt-2">
    <label class="small"> <span class="ico-info"></span></label>
    <div class="d-flex align-items-stretch justify-content-center">
      <input type="text" class="flex-grow-1">
      <div class="ico-plus fs14 p-2 ms-2 pointer hoverbg" data-tmpl="add"></div>
    </div>
  </div>
  <div data-tmpl="showOther" class="d-hide mt-2 pointer d-flex align-items-center justify-content-start">
    <span data-tmpl="showIcon" class="ico-plus fs8 ps-1"></span>
    <span data-tmpl="hideIcon" class="ico-minus fs8 ps-1"></span>
    <span data-tmpl="showHideMsg" class="d-inline-block ps-1 pb-1"></span>
  </div>
  <div data-tmpl="otherSettings" class="d-hide mt-3">
    <div data-tmpl="loadedSettingsMsg" class="fs15 pt-3 pb-1 ps-1">&mdash; [[[loaded from file]]] &mdash;</div>
    <div data-tmpl="loadedSettings" class="dynamicopts"></div>
    <div data-tmpl="defaultSettingsMsg" class="fs15">&mdash; [[[defaults]]] &mdash;</div>
    <div data-tmpl="defaultSettings" class="dynamicopts"></div>
  </div>
</div>
<div data-tmpl="errMsg" class="fs15 pt-3 text-center d-hide text-danger text-break"></div>
{{end}}

{{define "walletCfgGuideTemplate"}}
<a data-tmpl="walletCfgGuide" class="d-flex justify-content-start align-items-center pt-2 mx-3 pointer hoverbg" target="_blank">
  <span class="ico-textfile me-2"></span>
  <span class="pointer">[[[configuration guide]]]</span>
</a>
{{end}}

{{define "newWalletForm"}}
<div class="form-closer"><span class="ico-cross"></span></div>
<header>
  <span class="fs14 ico-plus grey me-2"></span>
  <span>[[[add_a_x_wallet]]]</span>
</header>
<div class="flex-center pb-2 border-bottom d-hide" data-tmpl="tokenMsgBox">
  <span>[[[Token on]]]</span>
  <img data-tmpl="tokenParentLogo" class="ms-1 micro-icon">
  <span data-tmpl="tokenParentName"></span>
</div>
<div data-tmpl="walletTypeTabs" class="d-flex justify-content-stretch border-bottom p-0">
  <div data-tmpl="walletTabTmpl" class="wtab px-3 py-2 border-end user-select-none"></div>
</div>
<div class="position-relative" data-tmpl="mainForm">
  <div class="flex-center pt-3" data-tmpl="oneBttnBox">
    <button class="one-bttn w-auto" data-tmpl="oneBttn">[[[Create]]]!</button>
  </div>
  <div data-tmpl="goBack" class="pt-3 d-flex align-items-center fs14 grey d-hide">
    <span class="ico-arrowleft fs12 me-1"></span>
    <span>[[[pick a different asset]]]</span>
  </div>
  <div data-tmpl="walletSettings" class="pt-3">
    {{template "walletConfigTemplates"}}
  </div>
  {{template "walletCfgGuideTemplate"}}
  <div class="d-flex align-items-end flex-wrap mt-3" data-tmpl="walletPassAndSubmitBttn">
    <div class="flex-grow-1 me-3" data-tmpl="newWalletPassBox">
      <label for="newWalletPass">[[[Wallet Password]]]
        <span class="ico-info" data-tooltip="[[[w_password_tooltip]]]"></span>
      </label>
      <input type="password" data-tmpl="newWalletPass" autocomplete="off">
    </div>
    <button data-tmpl="submitAdd" type="submit" class="feature flex-grow-1">[[[Add]]]</button>
  </div>
  <div class="p-3 text-center d-hide text-danger text-break" data-tmpl="newWalletErr"></div>
</div>
<div class="d-hide flex-center flex-column" data-tmpl="parentSyncing">
  <div>
    [[[Synchronizing]]] <img data-tmpl="parentLogo" class="micro-icon"> <span data-tmpl="parentName"></span>.
  </div>

  <div class="fs20 pt-3">
    <span data-tmpl="parentSyncPct"></span> %
  </div>
  <div class="pt-3">
    <img data-tmpl="childLogo" class="micro-icon"> <span data-tmpl="childName"></span> [[[wallet_wait_synced]]].
  </div>
</div>
{{end}}

{{define "depositAddress"}}
<div class="form-closer"><span class="ico-cross"></span></div>
<header>
  [[[Receive]]]
  <img id="depositLogo" class="mini-icon mx-1">
  <span id="depositName"></span>
</header>
<div class="flex-center d-hide fs18 sans pb-3 border-bottom" id="depositTokenMsgBox">
  <span>[[[Token on]]]</span>
  <img id="depositTokenParentLogo" class="ms-1 micro-icon">
  <span id="depositTokenParentName"></span>
</div>
<div class="flex-center">
  <img id="qrcode" />
</div>
<div>
  <div class="mono fs15 px-2 py-1 border word-break-all border">
    <span id="depositAddress" class="user-select-all"></span>
    <span id="copyAddressBtn" class="ico-copy mt-2 ml-2 fs18 pointer"></span>
    <span id="copyAlert" class="d-hide text-nowrap">[[[copied]]]</span>
  </div>
</div>

<div id="unifiedReceivers" class="d-flex align-items-stretch">
  <div id="unifiedReceiverTmpl" class="p-1 me-2 hoverbg lh1 fs15 pointer brdr selectable"></div>
</div>
<div id="newDepAddrBttnBox" class="flex-stretch-column">
